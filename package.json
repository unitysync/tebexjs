{
	"name": "@unitysync/tebexjs",
	"version": "0.1.1",
	"description": "A JavaScript SDK for interacting with the Tebex API",
	"type": "module",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			},
			"require": {
				"types": "./dist/index.d.cts",
				"default": "./dist/index.cjs"
			}
		}
	},
	"sideEffects": false,
	"files": [
		"dist",
		"package.json"
	],
	"bugs": {
		"url": "https://github.com/unitysync/tbx.js/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/unitysync/tbx.js.git"
	},
	"scripts": {
		"lint": "biome lint --write .",
		"format": "biome format --write .",
		"check": "biome check --write .",
		"clean": "pnpm rimraf dist",
		"typecheck": "tsc --noEmit",
		"build": "pnpm clean && tsup",
		"prepare": "husky"
	},
	"keywords": [
		"tebex"
	],
	"author": "unitysync",
	"license": "MIT",
	"devDependencies": {
		"@biomejs/biome": "^2.1.4",
		"husky": "^9.1.7",
		"rimraf": "^6.0.1",
		"tsc-alias": "^1.8.16",
		"tsup": "^8.5.0",
		"typescript": "^5.9.2"
	}
}
