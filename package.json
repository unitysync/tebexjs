{
	"name": "tbx.js",
	"version": "1.0.0",
	"description": "A JavaScript SDK for interacting with the Tebex API",
	"type": "module",
	"main": "./dist/cjs/index.js",
	"module": "./dist/esm/index.js",
	"types": "./dist/dts/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/esm/index.js",
			"require": "./dist/cjs/index.js"
		}
	},
	
	"bugs": {
		"url": "https://github.com/unitysync/tbx.js/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/unitysync/tbx.js.git"
	},
	"scripts": {
		"format": "biome format --write .",
		"lint": "biome lint .",
		"clean": "rimraf dist",
		"build:types": "tsc && tsc-alias",
		"build": "pnpm clean && tsx ./scripts/build.ts && pnpm build:types",
		"prepare": "pnpm build"
	},
	"keywords": [
		"tebex"
	],
	"author": "unitysync",
	"license": "MIT",
	"devDependencies": {
		"@biomejs/biome": "^2.1.4",
		"esbuild": "^0.25.9",
		"rimraf": "^6.0.1",
		"tsc-alias": "^1.8.16",
		"tsx": "^4.20.4",
		"typescript": "^5.9.2"
	},
	"engines": {
		"node": ">=18"
	}
}
